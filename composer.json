{
	"name": "simpod/phpstan-graphql",
	"description": "PHPStan GraphQL extension",
	"license": "MIT",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"type": "phpstan-extension",
	"extra": {
		"phpstan": {
			"includes": [
				"extension.neon"
			]
		}
	},
	"require": {
		"php": "~7.1",
		"phpstan/phpstan": "^0.10|^0.11|^0.12",
		"phpstan/phpdoc-parser": "^0.3",
		"nikic/php-parser": "^4.0",
		"webonyx/graphql-php": "^0.13.0"
	},
	"require-dev": {
		"doctrine/coding-standard": "^5.0",
		"jakub-onderka/php-parallel-lint": "^1.0",
		"phing/phing": "^2.16.0",
		"phpstan/phpstan-strict-rules": "^0.10",
		"phpstan/phpstan-phpunit": "^0.10",
		"phpunit/phpunit": "^7.2"
	},
	"autoload": {
		"psr-4": {
			"PHPStan\\": "src/"
		}
	},
	"autoload-dev": {
		"classmap": ["tests/"]
	}
}
